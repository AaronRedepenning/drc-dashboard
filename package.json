{
  "name": "drc-dashboard",
  "version": "1.0.0",
  "description": "A Web Dashboard for DRC's building automation project",
  "scripts": {
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "start": "./node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src"
  },
  "contributors": [
    "Aaron Redepenning <rede3704@stthomas.edu>"
  ],
  "devDependencies": {
    "awesome-typescript-loader": "0.15.10",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.33.3",
    "html-loader": "^0.4.3",
    "reflect-metadata": "0.1.2",
    "typescript": "~1.8.7",
    "typings": "^0.7.4",
    "webpack": "^1.12.9",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.3"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "chartist": "^0.9.7",
    "core-js": "^2.2.0",
    "d3": "^3.5.16",
    "heatmapjs": "git+https://github.com/pa7/heatmap.js.git",
    "leaflet": "^0.7.7",
    "normalize.css": "^4.0.0",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AaronRedepenning/drc-dashboard.git"
  }
}
